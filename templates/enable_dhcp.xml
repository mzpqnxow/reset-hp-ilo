<RIBCL VERSION="2.0">
<LOGIN USER_LOGIN="%%USERNAME%%" PASSWORD="%%PASSWORD%%">
<RIB_INFO MODE="WRITE" >
<MOD_NETWORK_SETTINGS>
<IP_ADDRESS VALUE = "0.0.0.0"/>
<SUBNET_MASK VALUE = "0.0.0.0"/>
<GATEWAY_IP_ADDRESS VALUE = "0.0.0.0"/>
<PRIM_DNS_SERVER value = "0.0.0.0"/>
<DHCP_ENABLE VALUE = "Y"/>
<DHCP_GATEWAY VALUE = "Y"/>
</MOD_NETWORK_SETTINGS>
</RIB_INFO>
</LOGIN>
</RIBCL>
